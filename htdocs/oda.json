$.ajax({
	url: 'test.json',
	dataType: 'json',
	success: function (data) {
		console.log("data", data);
		var dataArray = data.month;

		$.each(dataArray, function (i) {
			$(".flowerName").append(dataArray[i].name);
		});
	}
});


