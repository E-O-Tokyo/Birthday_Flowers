$(function(){
  $("#modal-close").click(function(){
    $("box").keyframes({
      '50%': {
        translateX:250,
        rotate:0
      },

      '100%': {
        translateX:500,
        rotate:360
      }
    },
    {
      duration: 2000,
      easing: 'ease',
      count: 2,
      direction: 'alternate'
    });
  });
});


$(window).load(function() {
  $('.modal-content').flexslider({
    animation: "slide"
  });
});


$.ajax({
	url: 'test.json',
	dataType: 'json',
	success: function(data){
    console.log("data",data);
		var dataArray = data.month;

		$.each(dataArray, function(i){
			$(".flowerName").append(dataArray[i].name);
		});
	}
});


