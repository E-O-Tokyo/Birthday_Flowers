function addList() {
	var html = "";
	for (var day_i = 1; day_i < 31; day_i++) {
		var paddingDay = ('0' + day_i).slice(-2);
		var list = '<li>' +
			'<div class="image_box">' +
			'<a class="modal-syncer button-link-01" data-target="modal-content-' + paddingDay + '" title="フクジュソウ">' +
			'<img src="img/resize/01' + paddingDay + '_resize.jpg" alt="" class="image">' +
			'</a>' +
			'<p class="date">' + paddingDay + '</p>' +
			'</div>' +
			'</li>';
		html += list;
	}
	$(".flowers-image").html(html);
}


